<?xml version="1.0" encoding="utf-8"?>
    <androidx.appcompat.widget.LinearLayoutCompat
        xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".CommanderDamageFragment">
        <GridLayout
            android:columnCount="2"
            android:rowCount="2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="1dp"
            tools:context=".CommanderDamageFragment">

            <!--row 0 col 0-->
            <androidx.cardview.widget.CardView
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:rotation="180">
                <LinearLayout
                    android:id="@+id/linearLayoutView1ID"
                    android:layout_width="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:id="@+id/commanderDamageView1ID"
                        android:layout_width="match_parent"
                        android:layout_height="32dp"
                        android:autoSizeTextType="uniform"
                        android:layout_gravity="center_horizontal"
                        android:textAlignment="center" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearLayoutButtonGrid2ID"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_gravity="center_horizontal|center_vertical">
                    <Button
                        android:id="@+id/decreaseCommanderDamageCountButtonView1ID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal"
                        android:text="-"
                        android:textSize="@dimen/buttonTextSize"
                        style="@style/Widget.MaterialComponents.Button"/>
                    <Button
                        android:id="@+id/increaseCommanderDamageCountButtonView1ID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:text="+"
                        android:textSize="@dimen/buttonTextSize"
                        style="@style/Widget.MaterialComponents.Button"/>

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <!--row 0 col 1-->

            <androidx.cardview.widget.CardView
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:rotation="180">
                <LinearLayout
                    android:id="@+id/linearLayoutView2ID"
                    android:layout_width="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:id="@+id/commanderDamageView2ID"
                        android:layout_width="match_parent"
                        android:layout_height="32dp"
                        android:layout_gravity="center_horizontal"
                        android:textAlignment="center" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearLayoutView2ButtonID"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_gravity="center_horizontal|center_vertical">
                    <Button
                        android:id="@+id/decreaseCommanderDamageCountButtonView2ID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal"
                        android:text="-"
                        android:textSize="@dimen/buttonTextSize"
                        style="@style/Widget.MaterialComponents.Button"/>
                    <Button
                        android:id="@+id/increaseCommanderDamageCountButtonView2ID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:textSize="@dimen/buttonTextSize"
                        android:text="+"
                        style="@style/Widget.MaterialComponents.Button"/>

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_rowWeight="1"
                android:layout_columnWeight="1">
                <LinearLayout
                    android:tag="@+id/linearLayoutView3ID"
                    android:layout_width="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:id="@+id/commanderDamageView3ID"
                        android:layout_width="match_parent"
                        android:layout_height="32dp"
                        android:layout_gravity="center_horizontal"
                        android:textAlignment="center" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearLayoutView3ButtonID"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_gravity="center_horizontal|center_vertical">
                    <Button
                        android:id="@+id/decreaseCommanderDamageCountButtonView3ID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal"
                        android:textSize="@dimen/buttonTextSize"
                        android:text="-"
                        style="@style/Widget.MaterialComponents.Button"/>
                    <Button
                        android:id="@+id/increaseCommanderDamageCountButtonView3ID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:textSize="@dimen/buttonTextSize"
                        android:text="+"
                        style="@style/Widget.MaterialComponents.Button"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <!--row 1 col 0-->

            <!--row 1 col 1-->

            <androidx.cardview.widget.CardView
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                app:cardElevation="18dp">
                <LinearLayout
                    android:id="@+id/linearLayoutView4ID"
                    android:layout_width="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal">
                    <TextView
                        android:id="@+id/commanderDamageView4ID"
                        android:layout_width="match_parent"
                        android:layout_height="32dp"
                        android:layout_gravity="center_horizontal"
                        android:textAlignment="center" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearLayout2View4ID"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_gravity="center_horizontal|center_vertical">
                    <Button
                        android:id="@+id/decreaseCommanderDamageCountButtonView4ID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center_horizontal"
                        android:text="-"
                        android:textSize="@dimen/buttonTextSize"
                        style="@style/Widget.MaterialComponents.Button"/>
                    <Button
                        android:id="@+id/increaseCommanderDamageCountButtonView4ID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:textSize="@dimen/buttonTextSize"
                        android:text="+"
                        style="@style/Widget.MaterialComponents.Button"/>

                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </GridLayout>
</androidx.appcompat.widget.LinearLayoutCompat>